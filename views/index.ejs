								<div class="item"># 코미디</div>
								<div class="item"># 엔터테인먼트</div>
								<div class="item"># 뉴스/정치</div>
								<div class="item"># 노하우/스타일</div>
								<div class="item"># 교육</div>
								<div class="item"># 과학기술</div>
								<div class="item"># 비영리/사회운동</div>
							</div>
						</div>
					</div>
			</div>
		</section>

		<section id="items" class="section">
			<div class="container">
				<div class="item_list">
					<a href="/tagging" target="_blank"><div class="item item_first"><img src="/images/index/hashtag.png" class="icon"><h5 class="title">분석</h5><div class="button">바로가기</div></div></a>
					<!-- <a href="/category" target="_blank"><div class="item item_second"><img src="/images/index/interface.png" class="icon"><h5 class="title">분류</h5><div class="button">바로가기</div></div></a> -->
					<a href="/category" target="_blank"><div class="item item_third"><img src="/images/index/interface.png" class="icon"><h5 class="title">분류</h5><div class="button">바로가기</div></div></a>
				</div>
			</div>
		</section>


		<div id="analysis" class="section">
			<div class="container">
				<div class="section_list">
					<div class="item_section" id="section_1">
						<div class="section_title">
							# 스포츠 ⚽️
							<div class="buttons">
								<div class="button left"></div>
								<div class="button right"></div>
							</div>
						</div>
						<p class="section_subtitle"></p>
						<div class="item_list" id="main_sports" style="width: 830px; left: 0px;">
						</div>
					</div>
					<div class="item_section" id="section_2">
						<div class="section_title">
							# 게임 🎮
							<div class="buttons">
